#ifndef CTIMEOUT_H_
#define CTIMEOUT_H_

#include "IEvent.h"

namespace rm
{

using namespace std;

class CTimeOut: public IEvent
{
public:
	CTimeOut();
	~CTimeOut();

	virtual int8_t OnCreate();
	virtual int8_t OnRead();
	virtual int8_t OnWrite();
	virtual int8_t WaitEvents();
	virtual int8_t OnTimeOut();

private:
	std::list<IEvent*> m_eventlist;
};

}

#endif /* CTIMEOUT_H_ */
