#include "CTimeOut.h"

namespace rm
{

CTimeOut::CTimeOut()
{
}

CTimeOut::~CTimeOut()
{
	ITER iter;
	for (iter = m_eventlist.begin(); iter != m_eventlist.end(); ++iter)
	{
		delete (*iter);
	}
}

int8_t CTimeOut::OnCreate()
{
	return 0;
}
int8_t CTimeOut::OnRead()
{
	int64_t diff = 0, min = LONG_MAX;
	unint64_t cutt = 0;
	ITER iter;
	IEvent *mod = NULL;
	for (iter = m_eventlist.begin(); iter != m_eventlist.end(); ++iter)
	{
		mod  = *iter;
		if (NULL == mod)
		{
			continue;
		}


	}
}
int8_t CTimeOut::OnWrite()
{
	return 0;
}
int8_t CTimeOut::WaitEvents()
{
	return 0;
}

}
